  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Get Project",
      "type": "python",
      "request": "launch",
      "program": "src/octopus_python_client/main.py",
      "console": "integratedTerminal",
      "justMyCode": true,
      "args": [
        "-d=${workspaceFolder}/generated/identity-adhoc-core",
        "-k=", 
        "-o=https://octopus.vnerd.com/api/",

        "-a=get_project",
        "-pj=identity-adhoc-core",
      ],
    },
    {
      "name": "Python: Transform Project",
      "type": "python",
      "request": "launch",
      "program": "src/data_transforms/main.py",
      "console": "integratedTerminal",
      "justMyCode": true,
      "args": [
        "-p=identity-web-core",
        "-id=${workspaceFolder}/generated/",
        "-d=${workspaceFolder}/generated/converted/",
      ],
    }
  ]
}